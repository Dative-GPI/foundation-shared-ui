    "@fontsource/montserrat": "*",
    "@lexical/clipboard": "^0.12.5",
    "@lexical/history": "^0.12.5",
    "@lexical/link": "^0.12.5",
    "@lexical/plain-text": "^0.12.5",
    "@lexical/rich-text": "^0.12.5",
    "@lexical/selection": "^0.12.5",
    "@lexical/text": "^0.12.5",
    "@lexical/utils": "^0.12.5",
    "@mdi/font": "7.4.47",
