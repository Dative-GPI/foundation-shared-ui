{
  "name": "storybook",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint . --fix --ignore-path .gitignore",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "install-peers": "for mod in $(jq -r 'keys[]' package.json | xargs -I {} npm info {} peerDependencies | jq -r 'to_entries | .[] | .key + \"@\" + .value'); do npm install $mod; done"
  },
